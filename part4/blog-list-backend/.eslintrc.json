{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {
     "endOfLine": "auto",
     "singleQuote": true
   }],
    "no-console": 0,
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "import/first": "off",
    "quotes": ["error", "single"]
  }
}