{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": 0,
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "import/first": "off",
    "react/prop-types": "off",
    "react/jsx-one-expression-per-line": "off"
  },
  "globals": {
    "window": true,
    "document": true
  }
}
